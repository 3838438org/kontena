json.from_time @node_stats[:from_time]
json.to_time @node_stats[:to_time]
json.total_data_points @node_stats[:total_data_points]
json.average_cpu_usage_percent @node_stats[:average_cpu_usage_percent]
json.average_memory_used_bytes @node_stats[:average_memory_used_bytes]
json.average_memory_total_bytes @node_stats[:average_memory_total_bytes]
json.average_memory_used_percent @node_stats[:average_memory_used_percent]
json.average_filesystem_used_bytes @node_stats[:average_filesystem_used_bytes]
json.average_filesystem_total_bytes @node_stats[:average_filesystem_total_bytes]
json.average_filesystem_used_percent @node_stats[:average_filesystem_used_percent]
